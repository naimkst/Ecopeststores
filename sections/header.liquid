<header class="header">
    <style>
        .search-section {
            position: absolute;
            z-index: 999;
            background: #fff;
            width: 100%;
            padding: 25px;
        }

        .thumbnail {
            display: inline-block;
            max-width: 30%;
            float: left;
        }

        .title {
            display: inline-block;
            max-width: 70%;
            margin-left: 10px;
        }

        ul.search-result li {
            padding: 12px 0px;
            border-bottom: 1px solid #f3f3f3;
            display: flow-root;
        }

        ul.search-result li:last-child {
            padding: 12px 0px;
            border-bottom: 0px solid #f3f3f3;
        }

        li.all-result {
            background: #f13a33;
            padding: 18px 0px !important;
            text-align: center;
            font-size: 18px;
            color: #fff !important;
            display: list-item;
            margin-top: 20px;
        }

        li.all-result a {
            color: #fff !important;
        }

        .title p {
            margin-top: 6px;
            font-size: 12px;
            color: #e73236;
        }

        .search-section {
            display: none;
        }

        ::-webkit-input-placeholder {
            color: #1C3D72;
        }

        .search-section {
            max-height: 500px;
            overflow: scroll;
        }
    </style>

    {% if section.settings.top_header_hide_show != false %}
    <!-- top bar -->
    <div class="top-bar">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-7">
                    <div class="owl-carousel owl-theme topbar-carousel">
                        <!-- item -->
                        {% for block in section.blocks %}
                        <div class="item">
                            <p>{{ block.settings.top_slider }}</p>
                        </div>
                        {% endfor %}
                        <!-- /item -->

                    </div>
                </div>
            </div>
        </div><!-- /container -->
        <!-- language -->
        <div class="lang">
            <div class="dropdown">
                <button class="btn dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown"
                    aria-haspopup="true" aria-expanded="false">
                    <img src="{{ 'ca.png' | asset_url }}" class="img-fluid" alt="">
                    CA
                </button>
                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                    <a class="dropdown-item" href="#">
                        <img src="{{ 'ca.png' | asset_url }}" class="img-fluid" alt="">
                        CA
                    </a>
                </div>
            </div>
        </div>
    </div>
    {% endif %}
    <!-- middle -->
    <div class="middle-header">
        <div class="container">
            <div class="row align-items-center justify-content-center">
                <div class="col-xl-3 col-md-4 col-sm-6 mb-xl-0 mb-3">
                    <!-- logo -->
                    {% if section.settings.logo != blank %}
                    <a href="/" class="logo">
                        <img src="{{ section.settings.logo | img_url: '' }}" class="img-fluid" alt="">
                    </a>
                    {% else %}
                    <a href="/" class="logo">
                        <img src="{{ 'logo.png' | asset_url }}" class="img-fluid" alt="">
                    </a>
                    {% endif %}
                </div>

                {% if section.settings.logo_right_image != blank %}
                <div class="col-xl-1 col-md-2 col-sm-3 mb-xl-0 mb-3">
                    <div class="logo-2">
                        <img src="{{ section.settings.logo_right_image | img_url: '' }}" class="img-fluid" alt="">
                    </div>
                </div>
                {% endif %}
                <div class="col-xl-4 col-md-10 mb-xl-0 mb-4">
                    <form action="/search" method="get" role="search" class="search-bar">
                        <input type="search" id="search-box" name="q" value="{{ search.terms | escape }}"
                            class="form-control typewrite form-control" onkeyup="ajaxSearch()" data-period="2000"
                            data-type='[ "Ecopeststore", "Ants", "Bed Bugs", "Wasps & Bees", "Spiders", "Mice", "Rats", "Voles", "Moles", "Cockroaches", "Sow Bugs", "Tree & Garden"]'>
                        <button type="submit" class="btn search-btn"><i class="fa fa-search"></i>
                            <span>search</span></button>
                    </form>

                    <div class="search-section">
                        <div class="search-loading"></div>
                        <ul class="search-result predictive-search-results">

                            <!-- <li class="all-result">
                                <a href="">
                                    View All Results (72)
                                </a>
                            </li> -->
                        </ul>
                    </div>
                </div>
                <div class="col-lg-4 col-md-10">
                    <ul class="middle-header-right">
                        {% if section.settings.sign_in_hide_show != false %}
                        <li>
                            <a href="{{ section.settings.sing_in }}" class="signin-card">
                                <img src="{{ section.settings.sign_in_image | img_url: '' }}" class="img-fluid" alt="">
                                Sign In
                            </a>
                        </li>
                        {% endif %}

                        {% if section.settings.cart_hide_show != false %}
                        <li>
                            <a href="/cart" class="cart-card">
                                <span class="cart-number">{{ cart.item_count }}</span>
                                <img src="{{ section.settings.Cart_image | img_url: '' }}" class="img-fluid" alt="">
                                Cart
                            </a>
                        </li>
                        {% endif %}

                        {% if section.settings.chat_hide_show != false %}
                        {% if section.settings.chat_image != blank %}
                        <li>
                            <a href="#" class="chat-card">
                                <img src="{{ section.settings.chat_image | img_url: '' }}" class="img-fluid" alt="">
                            </a>
                        </li>
                        {% endif %}

                        {% endif %}

                        {% if section.settings.video_hide_show != false %}
                        {% if section.settings.video_image != blank %}
                        <li>
                            <a data-fancybox href="{{ section.settings.header_video }}" class="video-card">
                                <img src="{{ section.settings.video_image | img_url: '' }}" class="img-fluid" alt="">
                            </a>
                        </li>
                        {% endif %}
                        {% endif %}
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- middle -->
    <!-- Navigation bar -->

