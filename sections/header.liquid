                            position: absolute;
                            content: '';
                            width: 100%;
                            height: 100%;
                            top: 0;
                            left: 0;
                            background: rgba(0, 0, 0, .2);
                        }

                        .know-more .menu-card h5 {
                            position: absolute;
                            left: 0;
                            right: 0;
                            text-align: center;
                            top: 50%;
                            transform: translateY(-50%);
                            z-index: 1;
                            margin: 0;
                            font-size: 22px;
                            font-weight: 500;
                            text-transform: capitalize;
                        }

                        .know-more {
                            background: #1C3D72;
                        }

                        .know-more li a {
                            color: #fff;
                            font-size: 16px;
                        }

                        .know-more h4 {
                            color: #fff;
                        }

                        .know-more .more-menu-width {
                            width: 80%;
                            margin: 0 auto;
                        }
                    </style>
                    <div class="container">
                        <div class="row">
                            <div class="more-menu-width">
                                <div class="col-lg-12">
                                    <div class="row">
                                        <div class="col-lg-12 mt-lg-0 mt-4">
                                            <div class="row">

                                                {% for link in linklists[section.settings.know_more_menu].links %}
                                                {% if link.lists == blank %}
                                                {% if forloop.index == 1 %}
                                                <div class="col-3">
                                                    <h4>{{ link.title }}</h4>
                                                    <ul>
                                                        {% for childlink in link.links %}
                                                        <li class="menu-item"><a href="{{ childlink.url }}">{{
                                                                childlink.title }} »</a></li>
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                                {% endif %}

                                                {% if forloop.index == 2 %}
                                                <div class="col-3">
                                                    <h4>{{ link.title }}</h4>
                                                    <ul>
                                                        {% for childlink in link.links %}
                                                        <li class="menu-item"><a href="{{ childlink.url }}">{{
                                                                childlink.title }} »</a></li>
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                                {% endif %}

                                                {% if forloop.index == 3 %}
                                                <div class="col-3">
                                                    <h4>{{ link.title }}</h4>
                                                    <ul>
                                                        {% for childlink in link.links %}
                                                        <li class="menu-item"><a href="{{ childlink.url }}">{{
                                                                childlink.title }} »</a></li>
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                                {% endif %}

                                                {% if forloop.index == 4 %}
                                                <div class="col-3">
                                                    <h4>{{ link.title }}</h4>
                                                    <ul>
                                                        {% for childlink in link.links %}
                                                        <li class="menu-item"><a href="{{ childlink.url }}">{{
                                                                childlink.title }} »</a></li>
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                                {% endif %}

                                                {% endif %}
                                                {% endfor %}
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-lg-4">
                    <div class="dropdown">
                        <button class="btn know-more-btn dropdown-toggle right-menu-toggole" type="button"
                            id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span>
                                Know More!
                                <span>Learn & Support</span>
                            </span>
                            <img src="{{ section.settings.right_menu_image | img_url: '' }}" class="img-fluid" alt="">
                        </button>
                        <div hidden class="dropdown-menu show" aria-labelledby="dropdownMenuButton">
                            {% for link in linklists[section.settings.know_more_menu].links %}
                            {% if link.lists == blank %}
                            <a class="dropdown-item" href="{{ link.url }}">{{ link.title }}</a>
                            {% endif %}
                            {% endfor %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



</header>

