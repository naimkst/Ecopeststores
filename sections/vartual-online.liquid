    .pro-service-box{
         width: 100%;
         height: auto;
         padding: 15px 15px 15px 15px;
         background: #FFFFFF;
         box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.25);
         text-align: center;
     }
    
