<section class="single-product">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">Home</a></li>
            <li class="breadcrumb-item active" aria-current="page">{{ product.title }}</li>
          </ol>
        </nav>
      </div>
    </div>


    <form action="/cart/add" method="post" enctype="multipart/form-data" id="AddToCartForm">
      <div class="row pb-5">
        
        <div class="col-md-4">
          <div class="spintrevoew spinBG">
          <div  id="loadingSingleProduct">
          </div>
          <div class="column small-11 small-centered">
            <div class="slider slider-single">
              {% for image in product.images %}
            <div data-thumb="{{ image.src | img_url: '' }}">
              <img class="img-fluid" src="{{ image.src | img_url: '' }}">
            </div>
            {%endfor %}
            </div>
            <div class="slider slider-nav">
              {% for image in product.images %}
            <div data-thumb="{{ image.src | img_url: '' }}">
              <img class="img-fluid" src="{{ image.src | img_url: '' }}">
            </div>
            {%endfor %}
            </div>
          </div>

        </div>

<!-- 
          <div class="single-product-images">

            {% for image in product.images %}
            <div data-thumb="{{ image.src | img_url: '' }}">
              <img class="img-fluid" src="{{ image.src | img_url: '' }}">
            </div>
            {%endfor %}

          </div> -->
        </div>
        <div class="col-md-7">
          <div class="single-product-details">
            <h2>{{ product.title }}</h2>
            <p class="product-price">{{ product.price | money }}</p>

            <p class="product-description">
              {{ product.description }}
            </p>

            {% unless product.has_only_default_variant %}
            {% for product_option in product.options_with_values %}
            <div class="product-category">
              <span>{{ product_option.name }}:</span>
              <select class="form-control" data-index="option{{forloop.index}}">

                {% for value in product_option.values %}
                <option value="{{ value | escape }}" {% if product_option.selected_value==value %} selected="selected"
                  {% endif %}>{{ value }}</option>
                {% endfor %}

              </select>
            </div>
            {% endfor %}
            {% endunless %}

            <div class="product-quantity">
              <span>Quantity:</span>
              <input min="1" type="number" id="quantity" name="quantity" value="1" />
            </div>
            <div class="product-category">
              {% if product.collections != blank %}
              <span>Categories:</span>
              <ul>
                {% for collection in product.collections %}
                <li><a href="{{ collection.url }}">{{ collection.title }}</a></li>
                {% endfor %}
              </ul>
              {% endif %}
            </div>
            <input type="hidden" id="variant-id" name="id" value="{{ product.variants.first.id }}">
            {% if collection.available == true %}
            <input type="submit" value="Add To Cart" class="shop-now-btn">
            {% else %}
            <a class="shop-now-btn disabled">Sold Out</a>
            {% endif %}
          </div>
        </div>
      </div>

      <div class="row">
        <div class="col-md-12">
          <ul class="nav nav-tabs" id="myTab" role="tablist">
            <li class="nav-item">
              <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home"
                aria-selected="true">Details</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" id="home-tab2" data-toggle="tab" href="#home2" role="tab" aria-controls="home2"
                aria-selected="true">How To Videos</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" id="home-tab3" data-toggle="tab" href="#home3" role="tab" aria-controls="home3"
                aria-selected="true">FAQ</a>
            </li>
            </li>
            <li class="nav-item">
              <a class="nav-link" id="home-tab4"  href="#shopify-product-reviews" >Reviews</a>
            </li>
          </ul>
          <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
              <p>{{ product.metafields.my_fields.long_description }}</p>
            </div>

            <div class="tab-pane fade" id="home2" role="tabpanel" aria-labelledby="home-tab2">
              <div class="row">
                <div class="col-md-7">

                <div class="csslider infinity" id="slider1">
                  {% for media in product.media %}
                    {% case media.media_type %}
                    {% when 'external_video' %}
                    <input type="radio" name="slides" checked="checked" id="slides_{{forloop.index}}"/>
                    {% when 'video' %}
                    <input type="radio" name="slides" checked="checked" id="slides_{{forloop.index}}"/>
                    {% else %}
                    {% endcase %}
                    {% endfor %}
                  <ul>
                    {% for media in product.media %}
                    {% case media.media_type %}
                    {% when 'external_video' %}
                    {{ media | external_video_tag | json }}
                    <li>
                      {{ media | external_video_tag: class: "youtube_video" }}
                    </li>
                    {% when 'video' %}
                    <<li>
                      {{ media | external_video_tag: class: "youtube_video" }}
                    </li>
                    {% else %}
                    {% endcase %}
                    {% endfor %}
                     
                  </ul>
                  <div class="arrows">

                    {% for media in product.media %}
                    {% case media.media_type %}
                    {% when 'external_video' %}
                    <label for="slides_{{forloop.index}}"></label>
                    {% when 'video' %}
                    <label for="slides_{{forloop.index}}"></label>
                    {% if forloop.first == true %}
                      <label class="goto-first" for="slides_{{forloop.index}}"></label>
                    {% endif %}

                    {% if forloop.last == true %}
                    <label class="goto-last" for="slides_{{forloop.index}}"></label>
                    {% endif %}

                    {% else %}
                    {% endcase %}
                    {% endfor %}
                    
                  </div>
                </div>

                </div>
                <div class="col-md-4">
                  <div class="video-right-content">
                    <h2>{{ product.metafields.my_fields.video_title }}</h2>
                    <p>{{ product.metafields.my_fields.video_description }}</p>
                  </div>
                </div>
              </div>
            </div>

            <div class="tab-pane fade" id="home3" role="tabpanel" aria-labelledby="home-tab3">
              <div class="accordion accordion-type-2 accordion-flush" id="accordion_2">
                {% for faq in product.metafields.my_fields.faqs.value %}
                <div class="card">
                  <div data-next-url="appication_{{ forloop.index }}" id="faq_id_{{ forloop.index }}"
                    class="card-header d-flex justify-content-between on-click-faq {% if forloop.index == 1 %} activestate {% endif %}">
                    <a role="button" data-toggle="collapse" href="#collapse_{{forloop.index}}i" {% if forloop.index==1
                      %} aria-expanded="false" {% endif %} class="collapsed">{% echo faq.question %}</a>
                  </div>
                  <div id="collapse_{{forloop.index}}i" class="collapse {% if forloop.index == 1 %} show {% endif %} "
                    data-parent="#accordion_2" role="tabpanel">
                    <div class="card-body pa-15">{% echo faq.ans %}</div>
                  </div>
                </div>
                {% endfor %}
              </div>
            </div>
          </div>
        </div>
      </div>
  </div>

  </form>
</section>

